{
  "name": "zcf-electron",
  "version": "1.0.0",
  "description": "5G CSV desktop analyzer",
  "main": "dist-electron/main/index.js",
  "author": "",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "set ELECTRON_RUN_AS_NODE=& electron-vite dev",
    "build": "electron-vite build",
    "preview": "set ELECTRON_RUN_AS_NODE=& electron-vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "dist:win": "npm run build && electron-builder --win nsis",
    "dist:win:publish": "npm run build && electron-builder --win nsis --publish always"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.2",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.3.9",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.8"
  },
  "build": {
    "appId": "com.zcf.csv-analyzer",
    "productName": "5G CSV Analyzer",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/renderer/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "asar": true,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "${env.GH_OWNER}",
        "repo": "${env.GH_REPO}"
      }
    ]
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
